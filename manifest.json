{
  "manifest_version": 2,
 
  "name": "CakePop",
  "description": "This extension will analyze a page and pull up various related links",
  "version": "1.0",
 
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html",
   "default_title": "Click Here"
  },
  "permissions": [
   "activeTab"
   ]
}